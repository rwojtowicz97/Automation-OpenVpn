#!/bin/bash
IPTABLES="/sbin/iptables"

$IPTABLES -F INPUT
ip6tables -F INPUT
$IPTABLES -F OUTPUT
ip6tables -F OUTPUT
$IPTABLES -F FORWARD
$IPTABLES -F -t nat

ETH_LOKALNA=bond0.3901
#ETH_LOKALNA4=eth1.3
#ETH_LOKALNA5=eth1.105
ETH_SWIAT=bond0.6
#ETH_SWIATVOIP=eth0.8
#ETH_SWIAT124=eth0.124
TUN_VPN=tun0

BIALYSTOK=85.232.247.35
LUBLIN=79.189.228.138
KOSZALIN=46.171.231.197  # STARY IP89.239.114.36 
KOSZALIN_STARY=89.239.114.36

IPIETRO=193.222.135.196
IPIETRO2=193.222.135.199
#VOIP_LAN=195.200.214.0/24
SIEC_GLOWNA=193.17.41.0/24
SIEC_VLAN1=193.222.135.0/25
SIEC_VLAN6=193.222.135.192/28
SIEC_VLAN5=193.222.135.224/27
SIEC_DP=194.0.171.128/25
SIEC_MH=212.77.98.0/24
MAEXT=212.77.100.89/32
BPGWP=212.77.105.0/24
WPIMG=212.77.99.26
ADVWP=212.77.100.127
REKWP=212.77.100.128
SGWP=212.77.98.9
AKCJESPEC=212.77.100.98
ALLADYN=212.77.100.106
PIXEL=212.77.100.84
FINSUPER=212.77.104.101
#ALLWP=212.77.96.0/19
NOCOWANIE=217.182.35.32/27
NOCOWANIE2=5.196.180.64/27
NOCOWANIE3=217.182.42.96/27
NOCOWANIE4=94.23.94.56/32
WAKACJE=212.77.99.97
WAKACJE2=212.77.99.99
WAKACJE3=212.77.99.73
WAKACJE4=212.77.99.92
VIBEZ=212.77.99.140
HBAZURE=172.17.7.133
HBAZURE2=172.17.7.134
HBAZURE3=172.17.7.132
#####################################   ZABEZPIECZENIA
# Don't accept source routed packets. Attackers can use source routing to generate
# traffic pretending to be from inside your network, but which is routed back along
# the path from which it came, namely outside, so attackers can compromise your
# network. Source routing is rarely used for legitimate purposes.

/bin/echo "0" > /proc/sys/net/ipv4/conf/all/accept_source_route

# Disable ICMP redirect acceptance. ICMP redirects can be used to alter your routing
# tables, possibly to a bad end.

for interface in /proc/sys/net/ipv4/conf/*/accept_redirects; do
   /bin/echo "0" > ${interface}
   done

# Enable bad error message protection.
/bin/echo "1" > /proc/sys/net/ipv4/icmp_ignore_bogus_error_responses


# Turn on reverse path filtering. This helps make sure that packets use
# legitimate source addresses, by automatically rejecting incoming packets
# if the routing table entry for their source address doesn't match the network
# interface they're arriving on. This has security advantages because it prevents
# so-called IP spoofing, however it can pose problems if you use asymmetric routing
# (packets from you to a host take a different path than packets from that host to you)
# or if you operate a non-routing host which has several IP addresses on different
# interfaces. (Note - If you turn on IP forwarding, you will also get this).
for interface in /proc/sys/net/ipv4/conf/*/rp_filter; do
   /bin/echo "1" > ${interface}
   done

# TMP NA CZAS MIGRACJI
#echo 0 > /proc/sys/net/ipv4/conf/eth1.3901/rp_filter
#echo 0 > /proc/sys/net/ipv4/conf/eth1.3/rp_filter

#Problem z dostępem do podsieci 10.3.38
echo 0 > /proc/sys/net/ipv4/conf/all/rp_filter
echo 0 > /proc/sys/net/ipv4/conf/bond0.3008/rp_filter
echo 0 > /proc/sys/net/ipv4/conf/bond0.6/rp_filter      
echo 0 > /proc/sys/net/ipv4/conf/bond0/rp_filter
echo 0 > /proc/sys/net/ipv4/conf/bond0.3901/rp_filter


# Log spoofed packets, source routed packets, redirect packets.
/bin/echo "1" > /proc/sys/net/ipv4/conf/all/log_martians


#
# logowanie
#   - wszystko wchodzace

#$IPTABLES -i $ETH_SWIAT -A FORWARD -s 0/0 -d 0/0 -p tcp --syn -m limit --limit 20/m -j LOG --log-prefix "FORWARD syn on $ETH_SWIAT "
#$IPTABLES -i $ETH_SWIAT -A INPUT -s 0/0 -d 0/0 -p tcp --syn -m limit --limit 20/m -j LOG --log-prefix "INPUT syn on $ETH_SWIAT "

#$IPTABLES -A FORWARD -s 193.17.41.117 -p tcp -m limit --limit 2000/m -j LOG --log-prefix "pbx117 <<---"

#$IPTABLES -A FORWARD -s 192.168.4.201 -p tcp -m limit --limit 2000/m -j LOG --log-prefix "svn -->"

### Domyslne polityki
$IPTABLES -P INPUT DROP
ip6tables -P INPUT DROP
$IPTABLES -P FORWARD DROP

### VPN - begin
# otwieramy port VPN na swiat
$IPTABLES -A INPUT -i lo -j ACCEPT
ip6tables -A INPUT -i lo -j ACCEPT
$IPTABLES -A INPUT -m state --state ESTABLISHED,RELATED -j ACCEPT
ip6tables -A INPUT -m state --state ESTABLISHED,RELATED -j ACCEPT
$IPTABLES -A INPUT -p icmp -j ACCEPT
ip6tables -A INPUT -p ipv6-icmp -m limit --limit 1/s -j ACCEPT
$IPTABLES -A INPUT -i $ETH_SWIAT -p udp --dport 1194 -j ACCEPT
ip6tables -A INPUT -i bond0.2000 -p udp --dport 1194 -j ACCEPT
$IPTABLES -A INPUT -i $ETH_SWIAT -s 193.222.135.0/24 -p tcp --dport 22 -j ACCEPT
# i na wewnetrzny, do testow
$IPTABLES -A INPUT -i $ETH_LOKALNA -p udp --dport 1194 -j ACCEPT
$IPTABLES -A INPUT -i $ETH_LOKALNA -p tcp --dport 22 -j ACCEPT
$IPTABLES -A INPUT -i $ETH_LOKALNA -d 193.17.41.56 -j ACCEPT #console2
$IPTABLES -A INPUT -i $ETH_LOKALNA -p tcp --dport 10050 -j ACCEPT

echo 1 > /proc/sys/net/ipv4/ip_forward

#bot z OVH - nieautoryzowana próba logowania
$IPTABLES -A INPUT -s 5.135.113.177 -j DROP # kazamat  

$IPTABLES -A FORWARD -m state --state ESTABLISHED,RELATED -j ACCEPT
$IPTABLES -A FORWARD -s 10.0.19.231 -d 10.29.16.246 -j ACCEPT ##karol psuje UPRAW-789
$IPTABLES -A FORWARD -s 10.32.92.231 -d 10.29.16.246 -j ACCEPT ##karol psuje UPRAW-789
$IPTABLES -A FORWARD -s 10.32.92.216 -d 10.29.15.162 -j ACCEPT ##UPRAW-999
$IPTABLES -A FORWARD -s 10.32.94.131 -d 10.29.16.246 -j ACCEPT ##PDPR-5968
$IPTABLES -A FORWARD -s 10.32.94.131 -d 10.29.16.198 -j ACCEPT ##SERVICE-16507

#$IPTABLES -A INPUT -i $ETH_SWIAT -s 193.17.41.0/24 -p tcp --syn -j ACCEPT

### NAT sieci vpn dla polaczen z adresami publicznymi WP
$IPTABLES -t nat -A POSTROUTING -o $ETH_SWIAT -s 10.29.0.0/19 -d $SIEC_GLOWNA -j MASQUERADE
$IPTABLES -t nat -A POSTROUTING -o $ETH_SWIAT -s 10.29.0.0/19 -d $SIEC_VLAN1 -j MASQUERADE
$IPTABLES -t nat -A POSTROUTING -o $ETH_SWIAT -s 10.29.0.0/19 -d $SIEC_VLAN6 -j MASQUERADE
$IPTABLES -t nat -A POSTROUTING -o $ETH_SWIAT -s 10.29.0.0/19 -d $SIEC_VLAN5 -j MASQUERADE
$IPTABLES -t nat -A POSTROUTING -o $ETH_SWIAT -s 10.29.0.0/19 -d $SIEC_MH -j MASQUERADE
$IPTABLES -t nat -A POSTROUTING -o $ETH_SWIAT -s 10.29.0.0/19 -d $MAEXT -j MASQUERADE
$IPTABLES -t nat -A POSTROUTING -o $ETH_SWIAT -s 10.29.0.0/19 -d $BPGWP -j MASQUERADE
$IPTABLES -t nat -A POSTROUTING -o $ETH_SWIAT -s 10.29.0.0/19 -d $WPIMG -j MASQUERADE
$IPTABLES -t nat -A POSTROUTING -o $ETH_SWIAT -s 10.29.0.0/19 -d $ADVWP -j MASQUERADE
$IPTABLES -t nat -A POSTROUTING -o $ETH_SWIAT -s 10.29.0.0/19 -d $REKWP -j MASQUERADE
$IPTABLES -t nat -A POSTROUTING -o $ETH_SWIAT -s 10.29.0.0/19 -d $AKCJESPEC -j MASQUERADE
$IPTABLES -t nat -A POSTROUTING -o $ETH_SWIAT -s 10.29.0.0/19 -d $SGWP -j MASQUERADE
$IPTABLES -t nat -A POSTROUTING -o $ETH_SWIAT -s 10.29.0.0/19 -d $ALLADYN -j MASQUERADE
$IPTABLES -t nat -A POSTROUTING -o $ETH_SWIAT -s 10.29.0.0/19 -d $PIXEL -j MASQUERADE
$IPTABLES -t nat -A POSTROUTING -o $ETH_SWIAT -s 10.29.0.0/19 -d $FINSUPER -j MASQUERADE
#$IPTABLES -t nat -A POSTROUTING -o $ETH_SWIAT -s 10.29.0.0/19 -d $ALLWP -j MASQUERADE
$IPTABLES -t nat -A POSTROUTING -o $ETH_SWIAT -s 10.29.0.0/19 -d $NOCOWANIE -j MASQUERADE
$IPTABLES -t nat -A POSTROUTING -o $ETH_SWIAT -s 10.29.0.0/19 -d $NOCOWANIE2 -j MASQUERADE
$IPTABLES -t nat -A POSTROUTING -o $ETH_SWIAT -s 10.29.0.0/19 -d $NOCOWANIE3 -j MASQUERADE
$IPTABLES -t nat -A POSTROUTING -o $ETH_SWIAT -s 10.29.0.0/19 -d $NOCOWANIE4 -j MASQUERADE
$IPTABLES -t nat -A POSTROUTING -o $ETH_SWIAT -s 10.29.0.0/19 -d $SIEC_DP -j MASQUERADE
$IPTABLES -t nat -A POSTROUTING -o $ETH_SWIAT -s 10.29.0.0/19 -d $WAKACJE -j MASQUERADE
$IPTABLES -t nat -A POSTROUTING -o $ETH_SWIAT -s 10.29.0.0/19 -d $WAKACJE2 -j MASQUERADE
$IPTABLES -t nat -A POSTROUTING -o $ETH_SWIAT -s 10.29.0.0/19 -d $WAKACJE3 -j MASQUERADE
$IPTABLES -t nat -A POSTROUTING -o $ETH_SWIAT -s 10.29.0.0/19 -d $WAKACJE4 -j MASQUERADE
$IPTABLES -t nat -A POSTROUTING -o $ETH_SWIAT -s 10.29.0.0/19 -d $VIBEZ -j MASQUERADE
$IPTABLES -t nat -A POSTROUTING -o $ETH_SWIAT -s 10.29.0.0/19 -d $HBAZURE -j MASQUERADE
$IPTABLES -t nat -A POSTROUTING -o $ETH_SWIAT -s 10.29.0.0/19 -d $HBAZURE2 -j MASQUERADE
$IPTABLES -t nat -A POSTROUTING -o $ETH_SWIAT -s 10.29.0.0/19 -d $HBAZURE3 -j MASQUERADE

# nowy lancuch, tylko na vpn:
$IPTABLES -F forward_vpn
$IPTABLES -X forward_vpn
$IPTABLES -N forward_vpn
$IPTABLES -A FORWARD -i $TUN_VPN -j forward_vpn

# DNS dla ludzi po VPN
#$IPTABLES -A FORWARD -o $TUN_VPN -p tcp -s 192.168.4.200 --sport 53 -j ACCEPT
#$IPTABLES -A FORWARD -o $TUN_VPN -p udp -s 192.168.4.200 --sport 53 -j ACCEPT

# NTP
$IPTABLES -A INPUT -i $ETH_LOKALNA -p udp --dport 123 -j ACCEPT

# ICMP
$IPTABLES -A forward_vpn -p icmp -j ACCEPT

# DNS
$IPTABLES -A forward_vpn -p udp -d 192.168.4.200 --dport 53 -j ACCEPT
$IPTABLES -A forward_vpn -p udp -d 10.10.129.181 --dport 53 -j ACCEPT
#$IPTABLES -A forward_vpn -p udp -d 10.10.5.127 --dport 53 -j ACCEPT
$IPTABLES -A forward_vpn -p udp -d 10.35.0.90 --dport 53 -j ACCEPT

# standardowe dostepy dla all
$IPTABLES -A forward_vpn -s 10.29.0.0/19 -d 10.10.5.42 -p tcp -m multiport --dports 80,443 -j ACCEPT # https://intranet.wp-sa.pl
$IPTABLES -A forward_vpn -s 10.29.0.0/19 -d 10.10.5.34 -p tcp -m multiport --dports 80,443,9444,19086 -j ACCEPT # spiid.int.wp-sa.pl
